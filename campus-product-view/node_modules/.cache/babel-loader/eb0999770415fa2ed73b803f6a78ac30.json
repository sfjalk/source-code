"sources":["src/views/admin/Interaction.vue"],"sourcesContent":["<template>\r\n    <div class=\"interaction-container\">\r\n      <!-- 搜索卡片 -->\r\n      <el-card class=\"search-card\" shadow=\"never\">\r\n        <div class=\"search-content\">\r\n          <el-input\r\n            v-model=\"interactionQueryDto.productId\"\r\n            placeholder=\"请输入商品ID\"\r\n            clearable\r\n            size=\"medium\"\r\n            style=\"width: 300px;\"\r\n            @clear=\"handleFilterClear\"\r\n            @keyup.enter=\"handleFilter\"\r\n          >\r\n            <template #append>\r\n              <el-button\r\n                type=\"primary\"\r\n                icon=\"el-icon-search\"\r\n                @click=\"handleFilter\"\r\n              ></el-button>\r\n            </template>\r\n          </el-input>\r\n        </div>\r\n      </el-card>\r\n  \r\n      <!-- 数据展示卡片 -->\r\n      <el-card class=\"data-card\" shadow=\"never\">\r\n        <!-- 表格区域 -->\r\n        <el-table\r\n          :data=\"tableData\"\r\n          stripe\r\n          style=\"width: 100%\"\r\n          :header-cell-style=\"{background: '#f5f7fa', color: '#606266'}\"\r\n        >\r\n          <el-table-column prop=\"userAccount\" label=\"账号\" width=\"120\"></el-table-column>\r\n          <el-table-column prop=\"userName\" label=\"用户名\" width=\"160\"></el-table-column>\r\n          <el-table-column prop=\"productId\" label=\"商品ID\" width=\"100\" sortable></el-table-column>\r\n          <el-table-column prop=\"type\" label=\"行为类型\" width=\"140\">\r\n            <template #default=\"{row}\">\r\n              <el-tag\r\n                :type=\"getActionTypeTag(row.type)\"\r\n                effect=\"plain\"\r\n                size=\"medium\"\r\n              >\r\n                {{ getActionTypeText(row.type) }}\r\n              </el-tag>\r\n            </template>\r\n          </el-table-column>\r\n          <el-table-column prop=\"productTitle\" label=\"商品名称\" min-width=\"200\"></el-tabl